{
  "grammars": [
    {
      "name": "html",
      "camelcase": "Html",
      "scope": "source.html",
      "path": ".",
      "external-files": [
        "src/tag.h"
      ],
      "file-types": [
        "html"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injections": [
        "queries/injections.scm"
      ],
      "injection-regex": "html"
    }
  ],
  "metadata": {
    "version": "0.20.2",
    "license": "MIT",
    "description": "HTML grammar for tree-sitter",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-html.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}